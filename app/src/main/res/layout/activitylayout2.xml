<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LL2">

    <Button
        android:id="@+id/InstagramBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:text="Instagram"
        app:layout_constraintBottom_toTopOf="@+id/BtnActBtn"
        app:layout_constraintEnd_toEndOf="@+id/BtnActBtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/BtnActBtn" />

    <Button
        android:id="@+id/BtnActBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ButtonAct"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.399" />

    <Button
        android:id="@+id/CalcBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Calcylator"
        app:layout_constraintEnd_toEndOf="@+id/BtnActBtn"
        app:layout_constraintStart_toStartOf="@+id/BtnActBtn"
        app:layout_constraintTop_toBottomOf="@+id/BtnActBtn" />
</androidx.constraintlayout.widget.ConstraintLayout>